{
  "name": "igloolab-backend",
  "version": "1.0.0",
  "description": "Backend API for igloolab - Pharmaceutical inventory management system",
  "main": "dist/index.mjs",
  "type": "module",
  "exports": "./dist/index.mjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.mjs",
    "build": "pkgroll",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run -d src/config/database.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/database.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/config/database.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d src/config/database.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d src/config/database.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typescript",
    "postgresql",
    "typeorm",
    "api",
    "rest",
    "igloolab",
    "backend"
  ],
  "author": "Cesar Londo√±o",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "pkgroll": "^2.20.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  }
}
